# players

Datatypes:
	getPlayerProperties(): Avatar/Item/Skill
	getName(): String
	getRank(): int
	getHealth(): int
	getEffectsOnOff(): ImageID # if index=-1 -> run whole ImageFile-animation
	getEffects(): (CompressedEffect[] in the packets, List<Effect> in the players)
	getPosition(): Position
	getImageID(): ImageID
	getPlayerStats(): PlayerStats/int[]/ints # not clear yet
	getCharges(): short[]
	getIP(): InetAddress

LobbyPlayer:
	*

MinimizedGamePlayer extends Entity:
# Client hat ein MinimizedGamePlayer für alle anderen Spieler
	getKD()
	getPlayerProperties()
	getName()
	getRank()
	getHealth()
	getEffectsOnOff()
	getPosition()
	getImageID()
	apply(MinimizedGamePlayerFrameUpdate)

MinimizedGamePlayerFrameUpdate:
	getHealth()
	getPosition()
	getImageID()

ExtendedGamePlayerFrameUpdate:
	getEffects() (accurat)
	getPlayerStats()
	getCharges()

ExtendedGamePlayer extends MinimizedGamePlayer:
# ServerGame hat ein ExtendedGamePlayer für alle Spieler
	getIP()
	getEffects() (accurat) # uses Effect-List of MinimizedGamePlayer
	getPlayerStats()
	getCharges()
	toMinimizedGamePlayerFrameUpdate()
	toExtendedGamePlayerFrameUpdate()
	apply(MinimizedGamePlayerFrameUpdate)
	apply(ExtendedGamePlayerFrameUpdate)
